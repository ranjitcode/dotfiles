#
# ~/.bashrc
#

# If not running interactively, don't do anything
[[ $- != *i* ]] && return
PS1='[\u@\h \W]\$ '
source $HOME/.bash_aliases
source $HOME/.asdf/asdf.sh
eval $(thefuck --alias)
eval "$(zoxide init bash)"


[ -f ~/.inshellisense/key-bindings.bash ] && source ~/.inshellisense/key-bindings.bash
