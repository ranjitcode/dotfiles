<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Add New Row">
        <Item>
          <IdeaActionID>Console.TableResult.AddRow</IdeaActionID>
          <ShortCut>'Alt+Insert'</ShortCut>
          <Description>Add New Row</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Cancel Query">
        <Item>
          <IdeaActionID>Console.TableResult.Cancel</IdeaActionID>
          <ShortCut>'Ctrl+2'</ShortCut>
          <Description>Cancel Query</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Collapse">
        <Item>
          <IdeaActionID>CollapseRegion</IdeaActionID>
          <ShortCut>'Ctrl+NumPad -' or 'Ctrl+Minus'</ShortCut>
          <Description>Collapse</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete Row">
        <Item>
          <IdeaActionID>Console.TableResult.DeleteRows</IdeaActionID>
          <ShortCut>'Ctrl+Y'</ShortCut>
          <Description>Delete Row</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete Rows">
        <Item>
          <IdeaActionID>Console.TableResult.DeleteRows</IdeaActionID>
          <ShortCut>'Ctrl+Y'</ShortCut>
          <Description>Delete Rows</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Duplicate">
        <Item>
          <IdeaActionID>DatabaseView.CopyAction</IdeaActionID>
          <ShortCut>'Ctrl+D'</ShortCut>
          <Description>Duplicate</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Execute">
        <Item>
          <IdeaActionID>Console.Jdbc.Execute</IdeaActionID>
          <ShortCut>'Ctrl+Enter'</ShortCut>
          <Description>Execute</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'Alt+Shift+7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Go to Declaration or Usages">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B'</ShortCut>
          <Description>Go to Declaration or Usages</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Query Console...">
        <Item>
          <IdeaActionID>Jdbc.OpenConsole.Any</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F10'</ShortCut>
          <Description>Jump to Query Console...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Modify Table...">
        <Item>
          <IdeaActionID>ModifyObject</IdeaActionID>
          <ShortCut>'Ctrl+6'</ShortCut>
          <Description>Modify Table...</Description>
          <Count>11</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Properties">
        <Item>
          <IdeaActionID>DatabaseView.PropertiesAction</IdeaActionID>
          <ShortCut>'Shift+Enter'</ShortCut>
          <Description>Properties</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reformat Code">
        <Item>
          <IdeaActionID>ReformatCode</IdeaActionID>
          <ShortCut>'Alt+Shift+L'</ShortCut>
          <Description>Reformat Code</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Refresh">
        <Item>
          <IdeaActionID>DatabaseView.Refresh</IdeaActionID>
          <ShortCut>'Ctrl+5'</ShortCut>
          <Description>Refresh</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reload Page">
        <Item>
          <IdeaActionID>Console.TableResult.Reload</IdeaActionID>
          <ShortCut>'Ctrl+5'</ShortCut>
          <Description>Reload Page</Description>
          <Count>19</Count>
        </Item>
      </Statistic>
      <Statistic Action="Remove">
        <Item>
          <IdeaActionID>DatabaseView.DropAction</IdeaActionID>
          <ShortCut>'Delete'</ShortCut>
          <Description>Remove</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'Shift+F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>10</Count>
          <Hit>4</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Submit">
        <Item>
          <IdeaActionID>Console.TableResult.Submit</IdeaActionID>
          <ShortCut>'Ctrl+Enter'</ShortCut>
          <Description>Submit</Description>
          <Count>11</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>2</Count>
          <Hit>4</Hit>
        </Item>
      </Statistic>
    </option>
  </component>
</application>